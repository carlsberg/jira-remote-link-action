{
  "name": "jira-remote-link-action",
  "version": "1.0.0",
  "description": "Creates remote links on Jira from GitHub issues",
  "main": "dist/index.js",
  "scripts": {
    "all": "npm run format && npm run build",
    "build": "ncc build index.js -o dist --source-map --license licenses.txt",
    "format": "prettier --write index.js",
    "format-check": "prettier --check index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlsberg/jira-remote-link-action.git"
  },
  "keywords": [
    "carlsberg",
    "github",
    "jira",
    "remote-link",
    "actions"
  ],
  "author": "Carlsberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlsberg/jira-remote-link-action/issues"
  },
  "homepage": "https://github.com/carlsberg/jira-remote-link-action#readme",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.29.1",
    "prettier": "^2.3.2"
  }
}
